{
  "name": "vue-admin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --mode dev",
    "build": "vue-cli-service build",
    "inspect": "vue-cli-service inspect --mode prod > webpack.js",
    "test:unit": "vue-cli-service test:unit",
    "eslint": "vue-cli-service lint --mode lint \"{src,build,mock}/**/*.{vue,js,ts,tsx}\" --fix",
    "prettier": "prettier --write \"{src,build,mock}/**/*.{js,json,jsx,ts,tsx,less,vue,html,md}\"",
    "lint": " yarn eslint && yarn prettier",
    "commit": "git add . && node_modules/.bin/cz",
    "push": "git push origin dev"
  },
  "dependencies": {
    "ant-design-vue": "^2.0.1",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "dayjs": "^1.10.3",
    "pinia": "^2.0.0-alpha.13",
    "vue": "^3.0.0",
    "vue-i18n": "^9.1.6",
    "vue-router": "^4.0.6"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-0",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-plugin-import": "^1.13.3",
    "commitizen": "^4.2.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.26.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.0.0-0",
    "git-revision-webpack-plugin": "^3.0.6",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4",
    "vue-jest": "^5.0.0-0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
